@model CQRSNight.CQRSDesingPattern.Results.CategoryResults.GetCategoryQueryResult
@{
    Layout = null;
}



<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<form method="post">
    <div class="container">
        <br /><br />
        <div class="alert alert-primary">Kategori Güncelleme Sayfası</div>
        <label>Kategori Id</label>
        <input type="text" asp-for="CategoryId" readonly class="form-control" />
        <br />
        <label>Kategori Adı</label>
        <input type="text" asp-for="CategoryName" class="form-control" />
        <br />
        <button class="btn btn-success">Kategoriyi Oluştur</button>
    </div>
</form>